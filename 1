<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>Assigment</title>
<body>

<script>

const books = [
    {
        id: " ", 
        chapter: " ",
    },
];

let output = '';

for(let i = 0; i < books.length; i++) {
    output += '<li>'+books[i].id+'<br>'+books[i].chapter+'<br>'</li>';
}

document.getElementById('content').innerHTML = output;

<script type="text/javascript" src="https://the-one-api.herokuapp.com/?ref=apilist.fun"></script>

</script>
</body> 
